{
  "extends": ["airbnb"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jasmine": true,
    "jest": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/no-named-as-default": "off",
    "react/prefer-stateless-function": "off",
    "import/no-duplicates": "off",
    "jsx-a11y/anchor-is-valid": "off"
  }
}